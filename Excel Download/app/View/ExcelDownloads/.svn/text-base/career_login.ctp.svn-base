<script language="javascript" type="text/javascript">

	
	   
	
	    try
			{
				  var getCanvas = document.getElementsByClassName('myCanvas');
					var getCanvasGreen = document.getElementsByClassName('myCanvasgreen');
					
					for(i = 0 ; i < getCanvas.length ; i++)
					{
						var canvas = document.getElementsByClassName('myCanvas')[i];
						var context = canvas.getContext('2d');
						context.beginPath();
						context.moveTo(26,0);
						context.lineTo(0,20);
						context.lineTo(26,20);
						context.strokeStyle = "#efefef" ;
						context.stroke();
						context.fillStyle = "#ffffff";
						context.fill();
						context.beginPath();
						context.moveTo(26,20);
						context.lineTo(26,0);
						context.strokeStyle = "#ffffff" ;
						context.stroke();
					}
					
					for(i = 0 ; i < getCanvasGreen.length ; i++)
					{
						var canvasG = document.getElementsByClassName('myCanvasgreen')[i];
						var contextG = canvasG.getContext('2d');
						contextG.beginPath();
						contextG.moveTo(0,0);
						contextG.lineTo(26,20);
						contextG.lineTo(0,26);
						contextG.strokeStyle = "#E5E7DC" ;
						contextG.stroke();
						contextG.fillStyle = "#f0f2e7";
						contextG.fill();
						contextG.beginPath();
						contextG.moveTo(26,20);
						contextG.lineTo(0,0);
						contextG.strokeStyle = "#f0f2e7" ;
						contextG.stroke();
					}
      
			}
		  catch(err)
		  {
				
		  alert("Your browser won't support html5 , Please use latest browser")
			
		  }
			
			
    </script>
